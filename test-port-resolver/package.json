{
  "name": "@tuulbelt/test-port-resolver",
  "version": "0.1.0",
  "description": "Concurrent test port allocation - avoid port conflicts in parallel tests",
  "main": "src/index.ts",
  "type": "module",
  "bin": {
    "portres": "./src/index.ts",
    "test-port-resolver": "./src/index.ts"
