  "keywords": [
    "tuulbelt",
    "port",
    "test",
    "concurrent",
    "allocation"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/tuulbelt/test-port-resolver.git"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "tsx": "^4.7.0",
    "typescript": "^5.3.0"
  }
}
