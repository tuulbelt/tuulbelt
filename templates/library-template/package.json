{
  "name": "@tuulbelt/library-name",
  "version": "0.1.0",
  "description": "One sentence description of what this library does",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "build": "tsc",
    "test": "node --import tsx --test test/index.test.ts",
    "test:watch": "node --import tsx --test --watch test/index.test.ts",
    "prepublishOnly": "npm run build",
    "dogfood": "npm run dogfood:flaky",
    "dogfood:flaky": "flaky --test 'npm test' --runs 10"
  },
  "keywords": [
    "tuulbelt",
    "library",
    "zero-dependencies"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/tuulbelt/library-name.git"
  },
  "bugs": {
    "url": "https://github.com/tuulbelt/tuulbelt/issues"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "devDependencies": {
    "@tuulbelt/test-flakiness-detector": "git+https://github.com/tuulbelt/test-flakiness-detector.git",
    "@types/node": "^20.0.0",
    "tsx": "^4.7.0",
    "typescript": "^5.3.0"
  }
}
